{
  "name": "timeslottr",
  "version": "0.1.0",
  "description": "Utility functions for working with timeslots.",
  "keywords": [
    "timeslot",
    "scheduler",
    "date"
  ],
  "license": "MIT",
  "author": "Tilo Mitra <tilomitra@gmail.com>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsup": "^8.0.1",
    "typescript": "^5.4.4",
    "vitest": "^1.5.0"
  }
}
